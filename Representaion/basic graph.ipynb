   "source": [
    "#making a basic graph\n",
    "#importing bokeh\n",
    "from bokeh.plotting import figure\n",
    "from bokeh.io import output_file,show\n",
    "\n",
    "#preapre some data\n",
    "x=[1,2,3,4,5]\n",
    "y=[5,6,7,2,1]\n",
    "\n",
    "#data prepare the output\n",
    "output_file(\"line.html\")\n",
    "\n",
    "#create figure objects\n",
    "f=figure()\n",
    "\n",
    "f.line(x,y)\n",
    "\n",
    "show(f)"
   ]
  }
